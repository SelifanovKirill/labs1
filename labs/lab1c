#include <stdio.h>
#include <math.h>

int main()
{
    double x, a, G, F, Y;

    printf("Введите Х:");
    scanf("%lf", &x);
    printf("Введите A:");
    scanf("%lf", &a);

    G = 5 * (-2 * a * a + a * x + 3 * x * x) / (10 * a * a + 11 * a * x + 3 * x * x);
    F = sin(10 * a * a - 7 * a * x + x * x);
    Y = atanh(45 * a * a - 79 * a * x + 30 * x * x);

    printf("G = %.4f\nF = %.4f\nY = %.4f", G, F, Y);
    return 0;
};
